{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.19/MicrosoftTeams.schema.json",
    "manifestVersion": "1.19",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "Tenant Tracker, by David McWee",
        "websiteUrl": "https://davidmcwee.com",
        "privacyUrl": "https://davidmcwee.com/tenant-tracker",
        "termsOfUseUrl": "https://davidmcwee.com/tenant-tracker"
    },
    "icons": {
        "color": "d.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "TenantTrkr${{APP_NAME_SUFFIX}}",
        "full": "Tenant Tracker by David McWee"
    },
    "description": {
        "short": "Tenant Tracker Agent",
        "full": "Tenant Tracker Agent that provides details of a tenant based on the openid connect configuration"
    },
    "accentColor": "#FFFFFF",
    "composeExtensions": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "copilotAgents": {
        "declarativeAgents": [
            {
                "id": "declarativeAgent",
                "file": "declarativeAgent.json"
            }
        ]
    },
    "validDomains": []
}
